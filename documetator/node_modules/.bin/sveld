#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/storage/Develope/svelte-components/node_modules/.pnpm/sveld@0.15.0/node_modules/sveld/node_modules:/mnt/storage/Develope/svelte-components/node_modules/.pnpm/sveld@0.15.0/node_modules:/mnt/storage/Develope/svelte-components/node_modules/.pnpm/node_modules:/mnt/storage/Develope/svelte-components/node_modules:/mnt/storage/Develope/node_modules:/mnt/storage/node_modules:/mnt/node_modules:/node_modules:/mnt/storage/Develope/svelte-components/documetator/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/storage/Develope/svelte-components/node_modules/.pnpm/sveld@0.15.0/node_modules/sveld/node_modules:/mnt/storage/Develope/svelte-components/node_modules/.pnpm/sveld@0.15.0/node_modules:/mnt/storage/Develope/svelte-components/node_modules/.pnpm/node_modules:/mnt/storage/Develope/svelte-components/node_modules:/mnt/storage/Develope/node_modules:/mnt/storage/node_modules:/mnt/node_modules:/node_modules:/mnt/storage/Develope/svelte-components/documetator/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/sveld@0.15.0/node_modules/sveld/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/sveld@0.15.0/node_modules/sveld/cli.js" "$@"
fi
